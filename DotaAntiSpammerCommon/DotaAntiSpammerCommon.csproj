        <AssemblyOriginatorKeyFile>..\..\signature.snk</AssemblyOriginatorKeyFile>
        <PublicSign>true</PublicSign>
    </PropertyGroup>

    <ItemGroup>
      <None Include="..\..\signature.snk">
        <Link>signature.snk</Link>
      </None>
    </ItemGroup>

</Project>
